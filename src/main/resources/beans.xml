<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 

<bean id="aobj" class="com.jfs.spring.SpringAutowire.A">
<property name="a" value="101"/>
<property name="msg" value="Hello Welcome"/>
</bean>
<bean id="bo" class="com.jfs.spring.SpringAutowire.B">
<constructor-arg value="100"/>
<constructor-arg value="Hii Good Morning!!"/>
</bean> 
<bean id="hello" class="com.jfs.spring.SpringAutowire.Hello" autowire="byType">

</bean>       


</beans>